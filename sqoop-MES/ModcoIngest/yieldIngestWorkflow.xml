<workflow-app name="ModcoIngest" xmlns="uri:oozie:workflow:0.1">
 	<start to="Do-Hive-Subflow"/>

    <action name="Do-Hive-Subflow">
    	<sub-workflow>
        	<app-path>/IncrementalYield/yieldIngestWorkflow-Hive.xml</app-path>
            <propagate-configuration/>
        </sub-workflow>
        <ok to="End-Ingest"/>
        <error to="End-Ingest"/>
    </action>


	<end name="End-Ingest"/>
</workflow-app>
